	SP EQU R15
	BP EQU R14
	STACKA EQU 0x1000
	INA EQU 0xFF00
	OUTA EQU 0xFF02
	LOADA EQU 0xFF10

	ORG LOADA
	START LOADA
	

DEBUT	LDW SP,#STACKA
	LDW R0,@INA
	STW R0,-(SP)
	LDW R0,#3
	STW R0,-(SP)
	LDW BP,SP
	JSR @DISTA
	LDW SP,BP
	STW R0,@OUTA
	JMP #DEBUT-$-2

ABS	LDW R0,(BP)+
	JAE #POSIT-$-2
	NEG R0,R0
POSIT	RTS

DISTA	STW R1,-(SP)
	LDW R0,(BP)+
	LDW R1,(BP)+
	SUB R1,R0,R0
	STW BP,-(SP)
	STW R0,-(SP)
	LDW BP,SP
	JSR @ABS
	LDW SP,BP
	LDW BP,(SP)+
	SRA R0,R0
	LDW R1,(SP)+
	RTS
